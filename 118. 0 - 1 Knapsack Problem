class Solution {
  public:
    int knapsack(int W, vector<int> &val, vector<int> &wt) {
        // code here
        int n = wt.size();
        
        vector<int> dp(W+1, 0);
        
        for(int i = 0 ; i<n ; i++){
            for(int j = W ; j >= wt[i] ; j--){
                dp[j] = max(dp[j], dp[j - wt[i]] + val[i]);
            }
        }
        
        return dp[W];
    }
};
