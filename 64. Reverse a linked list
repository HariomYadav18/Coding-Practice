/*
class Node {
 public:
    int data ;
    Node *next ;

    Node(int x) {
        data = x ;
        next = NULL ;
    }
};
*/

class Solution {
  public:
    Node* reverseList(Node* head) {
        // code here
        Node *temp = head, *prev = nullptr, *next;
        
        while(temp != NULL){
            
            next =  temp->next  ;
            temp->next = prev;
            prev = temp;
            temp = next;
        }
        return prev;
    }
};
