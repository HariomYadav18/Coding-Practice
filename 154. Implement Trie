// User function template for C++
class Trie {
  public:
    Trie* children[26];
    
    bool isLeaf;

    Trie() {
        // implement Trie
        isLeaf = false;
        for(int i = 0 ; i <26; i++){
            children[i] = nullptr;
        }
    }

    void insert(string &word) {
        // insert word into Trie
        Trie* node = this;
        for(char c : word){
            int idx = c - 'a';
            if(node->children[idx] == nullptr){
                node->children[idx] = new Trie();
            }
            node = node->children[idx];
        }
        node->isLeaf = true;
    }

    bool search(string &word) {
        // search word in the Trie
        
        Trie* node = this;
        for(char c : word){
            int idx = c - 'a';
            if(node->children[idx] == nullptr){
                return false;
            }
            node = node->children[idx];
        }
        return node->isLeaf;
    }

    bool isPrefix(string &word) {
        // search prefix word in the Trie
        Trie* node = this;
        for(char c : word){
            int idx = c - 'a';
            if(node->children[idx] == nullptr){
                return false;
            }
            node = node->children[idx];
            
            
        }
        return true;
        
    }
};
